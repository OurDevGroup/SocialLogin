/**
*   @output GUID : String
*/
importPackage( dw.system );

function execute( args : PipelineDictionary ) : Number
{
	args.GUID =	(S4() + S4() + "-" + S4() + "-4" + S4().substr(0,3) + "-" + S4() + "-" + S4() + S4() + S4()).toLowerCase();

   return PIPELET_NEXT;
}

function S4() {
    return (((1+Math.random())*0x10000)|0).toString(16).substring(1); 
}
 
